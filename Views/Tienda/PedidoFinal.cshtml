@model string[]

<h1 style="color:blue">Pedido realizado correctamente!!</h1>

<h2 style="color:green">
    Direccion del pedido: @ViewData["DIRECCION"]
</h2>
<p>Listado de productos comprados</p>

<ul class="list-group">
    @foreach (string dato in Model)
    {
        <li class="list-group-item">
            @dato
        </li>
    }
</ul>